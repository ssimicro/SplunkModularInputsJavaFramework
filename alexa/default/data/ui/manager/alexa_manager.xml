<endpoint name="data/inputs/alexa">
  <header>Alexa</header>
  <breadcrumb>
    <parent hidecurrent="False">datainputstats</parent>
    <name>Alexa</name>
  </breadcrumb>
  <elements>
    <element name="sourceFields" type="fieldset">
      <view name="list"/>
      <view name="edit"/>
      <view name="create"/>
      <elements>

           <element name="name" type="textfield" label="Stanza Name">
             <view name="list"/>
             <view name="create"/>
          
             <key name="exampleText">Name of this stanza</key>
           </element>

           <element name="https_port" type="textfield" label="HTTPS Port">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to 443</key>
           </element>
           <element name="https_scheme" type="textfield" label="HTTPS Scheme">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to https</key>
           </element>
           <element name="endpoint" type="textfield" label="Endpoint">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to www.splunkcowboy.com/alexa</key>
           </element>
           <element name="keystore" type="textfield" label="Keystore">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to SPLUNK_HOME/etc/apps/alexa/crypto/java-keystore.jks</key>
           </element>
           <element name="keystore_pass" type="password" label="Keystore Pass">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             
           </element>
           <element name="disable_request_signature_check" type="checkbox" label="Disable Request Signature Check">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to false</key>
           </element>

        
           <element name="supported_application_ids" type="textfield" label="Supported Application IDs">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
            
           </element>
           <element name="timestamp_tolerance" type="textfield" label="Timestamp Tolerance">
             <view name="list"/>
             <view name="create"/>
             <view name="edit"/>
             <key name="exampleText">Defaults to 150</key>
           </element>


</elements>
</element>
    
    
  
	<element name="eai:acl.app" label="App">
      <view name="list"/>
      <key name="processValueList">entity['eai:acl']['app'] or ""</key>
    </element>

  </elements>
</endpoint>